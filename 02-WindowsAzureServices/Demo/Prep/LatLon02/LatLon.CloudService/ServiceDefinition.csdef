<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="LatLon.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WebRole name="LatLon.WebRoleServiceHost" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="HttpBinding" endpointName="HttpEndpoint" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="HttpEndpoint" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
  </WebRole>
  <WorkerRole name="LatLon.WorkerRoleServiceHost" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <Endpoints>
      <InputEndpoint name="NetTcpServicePort" protocol="tcp" port="8001" />
      <InputEndpoint name="NetTcpMexPort" protocol="tcp" port="8002" />
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>